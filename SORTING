#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
bool compare(int c,int d){
    return c>d;
}
int main(){
    int n;
    cin>>n;
    vector <int> a(n);
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    sort(a.begin(),a.end(),compare);
    for(int i=0;i<n;i++){
        cout<<a[i];
    }
    return 0;
}
********************************************************************************************************************************
#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main(){
    int n;
    cin>>n;
    vector <int> a(n);
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    sort(a.begin(),a.end());
    for(int i=0;i<n;i++){
        cout<<a[i];
    }
    return 0;
}
